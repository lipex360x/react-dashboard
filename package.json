{
  "name": "react-boilerplate",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npx eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "preplopbuild": "rimraf plop/",
    "plopbuild": "tsup ./generators/plopfile.ts",
    "postplopbuild": "copyfiles -u 1 ./generators/**/*.hbs plop",
    "plop": "plop --plopfile ./plop/plopfile.js",
    "gen": "npm run plopbuild && npm run plop "
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@hookform/resolvers": "^2.9.7",
    "@mui/icons-material": "^5.10.14",
    "@mui/material": "^5.10.14",
    "@mui/x-data-grid": "^5.17.12",
    "axios": "^0.27.2",
    "immer": "^9.0.15",
    "phosphor-react": "^1.4.1",
    "polished": "^4.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.2",
    "react-hot-toast": "^2.3.0",
    "react-router-dom": "^6.3.0",
    "styled-components": "^5.3.5",
    "styled-media-query": "^2.1.2",
    "zod": "^3.18.0",
    "zustand": "^4.1.1"
  },
  "devDependencies": {
    "@rocketseat/eslint-config": "^1.1.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^2.0.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.22.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "plop": "^3.1.1",
    "rimraf": "^3.0.2",
    "tsup": "^6.5.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.7"
  }
}
